    "docs:dev": "vuepress dev .",
    "docs:build": "vuepress build .",
    "docs:server": "pm2 start .vuepress/server.js --name utils-doc-server",
    "release": "[ -e CHANGELOG.md ] && (HUSKY_SKIP_HOOKS=1 standard-version --skip.tag) || (HUSKY_SKIP_HOOKS=1 standard-version -f --skip.tag)",
