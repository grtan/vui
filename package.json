{
  "name": "@vivo/vui",
  "version": "1.5.1",
  "description": "A library of vue ui components and some tools",
  "main": "dist/lib.js",
  "module": "lib/index.js",
  "scripts": {
    "doc": "docsify serve ./ --port 80",
    "watch": "gulp watch",
    "build": "gulp",
    "lint": "standard --plugin html 'src/**/*.{js,vue}'",
    "fix": "standard --fix --plugin html 'src/**/*.{js,vue}'",
    "prepublishOnly": "npm run build",
    "precommit": "npm run lint",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.vmic.xyz/game-fed/vui.git"
  },
  "keywords": [
    "vivo",
    "ui",
    "component",
    "lib",
    "tool",
    "vue"
  ],
  "author": "greenhand",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime-corejs2": "^7.0.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0-beta.2",
    "css-element-queries": "^1.2.0",
    "intersection-observer": "^0.7.0",
    "photoswipe": "^4.1.3",
    "vue-runtime-helpers": "1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@vue/babel-preset-jsx": "^1.0.0-beta.2",
    "autoprefixer": "^9.4.9",
    "commitizen": "^3.0.7",
    "conventional-changelog-cli": "^2.0.12",
    "cz-conventional-changelog": "^2.1.0",
    "eslint-plugin-html": "^4.0.5",
    "fs-extra": "^7.0.1",
    "gulp": "^4.0.0",
    "gulp-base64": "^0.1.3",
    "gulp-base64-img": "^0.1.1",
    "gulp-clean-css": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-less": "^4.0.1",
    "gulp-modify-file": "^1.0.1",
    "gulp-pxtorem": "^3.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "less-plugin-autoprefix": "^2.0.0",
    "less-plugin-clean-css": "^1.5.1",
    "postcss-pxtorem": "^4.0.1",
    "postcss-url": "^8.0.0",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-url": "^2.2.0",
    "rollup-plugin-vue": "^4.7.1",
    "standard": "^11.0.1",
    "vue-template-compiler": "^2.6.8"
  },
  "sideEffects": [
    "./lib/assets/js/modernizr/modernizr.js"
  ],
  "standard": {
    "ignore": [
      "src/assets/js/**"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "> 0.5%",
    "last 50 version",
    "not ie <= 9"
  ]
}
