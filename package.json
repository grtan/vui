{
  "name": "@vivo/vui",
  "version": "1.14.0",
  "description": "A library of vue ui components and some tools",
  "main": "dist/lib.js",
  "module": "lib/index.js",
  "scripts": {
    "doc": "docsify serve ./ --port 80",
    "watch": "gulp watch",
    "build": "gulp",
    "lint": "eslint --ext .js,.vue ./src",
    "prepublishOnly": "npm run build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.vmic.xyz/game-fed/vui.git"
  },
  "keywords": [
    "vivo",
    "ui",
    "component",
    "lib",
    "tool",
    "vue"
  ],
  "author": "greenhand",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.5.5",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "css-element-queries": "^1.2.0",
    "intersection-observer": "^0.7.0",
    "photoswipe": "^4.1.3",
    "vue-runtime-helpers": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@vue/babel-preset-jsx": "^1.1.0",
    "autoprefixer": "^9.4.9",
    "commitizen": "^3.0.7",
    "conventional-changelog-cli": "^2.0.12",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "fs-extra": "^7.0.1",
    "gulp": "^4.0.0",
    "gulp-base64": "^0.1.3",
    "gulp-base64-img": "^0.1.1",
    "gulp-clean-css": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-less": "^4.0.1",
    "gulp-modify-file": "^1.0.1",
    "gulp-pxtorem": "^3.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-svg-symbols": "^3.2.3",
    "gulp-uglify": "^3.0.2",
    "husky": "^3.0.1",
    "klaw": "^3.0.0",
    "less-plugin-autoprefix": "^2.0.0",
    "less-plugin-clean-css": "^1.5.1",
    "postcss-pxtorem": "^4.0.1",
    "postcss-url": "^8.0.0",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-url": "^2.2.0",
    "rollup-plugin-vue": "^4.7.1",
    "vue-template-compiler": "^2.6.10"
  },
  "sideEffects": [
    "./lib/assets/js/**",
    "./lib/components/**/style/**/*.js",
    "./lib/tools/**/style.js"
  ],
  "eslintIgnore": [
    "/demo/",
    "/demo-old/",
    "/dist/",
    "/lib/",
    "/src/assets/"
  ],
  "husky": {
    "hooks": {}
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": [
    "> 0.5% and last 20 versions",
    "not Android < 4.0",
    "not ie < 10"
